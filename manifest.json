{
  "manifest_version": 2,
  "name": "Mi extensión",
  "version": "1.0",
  "description": "mi extensión pretende dar las prestaciones de chatgpt recibidas y expresadas por voz y animadas con avatares de inteligencia artifical",
  "icons": { "128": "icon.png" },
  "permissions": [
    "tabs",
    "activetab",
    "storage",
    "contextMenus",
    "notifications",
    "audioCapture",
    "videoCapture",
    "microphone",
    "camera",
    "https://*.openai.com/*",
    "http://*/*",
    "https://*/*",
    "unlimitedStorage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end",
      "all_frames": false
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "css/*",
        "images/*",
        "lib/*"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "externally_connectable": {
    "ids": [
      "jgjaeacdkonaoafenlfkkkmbaopkbilf",
      "eipjdkbchadnamipponehljdnflolfki",
      "pbajmiiebklfjhkeahpgjdlgclelihjh",
      "pjomkoghmkhnipjjlcgoncjlonoleckj",
      "ibeabbjcphoflmlccjgpebbamkbglpip",
      "lkjblpoingopdeaofcaapmeoojjjnhnc",
      "baecjmoceaobpnffgnlkloccenkoibbb",
      "daiacboceoaocpibfodeljbdfacokfjb",
      "meonalmakdjaojaoipfhahcfccoecegk",
      "mikcekmbahpbehdpakenaknkkedeonhf",
      "camppjleccjaphfdbohjdohecfnoikec",
      "debegnklcecggjmakkfjmbhikifklmjm"
    ],
    "matches": [
      "http://localhost/*",
      "https://localhost/*",
      "http://127.0.0.1/*",
      "https://127.0.0.1/*"
    ],
    "accept_tls_channel_id": true
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}




